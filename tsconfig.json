{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "downlevelIteration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es6",
    "incremental": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "sourceMap": true,
    "resolveJsonModule": true,
    "lib": [
      "es2019"
    ],
    "strict": true,
  },
  "exclude": [
    "node_modules"
  ]
}